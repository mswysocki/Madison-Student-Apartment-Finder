<section id="Landlord_View">
<h1 class="page_header">All Landlords</h1>
<hr class="generic_header_ruler" />

<%= will_paginate %>

	<table id="landlord-table"> 
		<tr>
			<th id="landlords-table-name">Name</th>
			<th id="landlords-table-email">Email</th>
			<th id="landlords-table-web">Website</th>
			<th id="landlords-table-phone">Phone Number</th>
			<th id="landlords-table-address">Address</th>
			<th id="landlords-table-city">City</th>
			<th id="landlords-table-zip">Zip</th>	
		</tr>
		<!-- Note: the render below has each tr inside of it -->
		<%= render @landlords %>
    </table>

<%= will_paginate %>
</section>
